{
  "version": "1.1.4.1",
  "description": "A library for bootstrapping microservices with support for actuator style endpoints.",
  "authors": [ "Lynxx131 <lynxx131@outlook.com>" ],
  "packOptions": {
    "tags": [ "api", "microservice", "vitals", "health check" ],
    "projectUrl": "https://github.com/lynxx131/dotnet.microservice",
    "licenseUrl": "https://github.com/lynxx131/dotnet.microservice/blob/master/LICENSE"
  },
  "title": "Dotnet Microservice Core",

  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "Microsoft.Extensions.Configuration": "1.0.0",
    "StackExchange.Redis": {
      "version": "1.1.604-alpha",
      "type": "build"
    },
    "Npgsql": {
      "version": "3.1.6",
      "type": "build"
    },
    "RavenDB.Client": {
      "version": "3.5.1",
      "type": "build"
    },
    "mongocsharpdriver": {
      "version": "2.4.0",
      "type": "build"
    }
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": ""
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Data": ""
      }
    },
    "netstandard1.6": {
      "imports": [
        "dnxcore50"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Diagnostics.Process": "4.1.0",
        "System.Net.NameResolution": "4.0.0",
        "System.Threading.Thread": "4.0.0",
        "system.data.sqlclient": {
          "version": "4.1.0",
          "type": "build"
        }
      }
    }
  }
}
